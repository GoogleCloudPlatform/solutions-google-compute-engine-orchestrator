<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns="http://java.sun.com/xml/ns/javaee"
xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
  <servlet>
    <servlet-name>Orchestrator</servlet-name>
    <servlet-class>com.google.cloud.solutions.sampleapps.orchestration.orchestrator.server.OrchestratorServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Orchestrator</servlet-name>
    <url-pattern>/orchestrator</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>InstanceStatus</servlet-name>
    <servlet-class>com.google.cloud.solutions.sampleapps.orchestration.orchestrator.server.InstanceStatusServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>InstanceStatus</servlet-name>
    <url-pattern>/instanceStatus</url-pattern>
  </servlet-mapping>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
  </welcome-file-list>
</web-app>
